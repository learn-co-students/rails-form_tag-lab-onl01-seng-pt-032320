<h3>Student Form</h3>
<%= form_tag students_path do %>
  <label>New Student Info:</label><br>
  <input type="text" id="new_student" name="student[first_name]"><br>
   <input type="text" id="new_student" name="student[last_name]"><br>
 
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="submit" value="Submit Student">
<% end %>
<%= params.inspect %>